{
    "name": "hydface/foundation",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": ">=5.4.0",
        "symfony/symfony": "~2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "3.7.*"
    },
    "autoload": {
        "psr-4": {
            "": "src/"
        }
    },
    "replace": {
        "hydface/foundation-bundle": "self.version",
        "hydface/upgrade-bundle": "self.version",
        "hydface/module-bundle": "self.version",
        "hydface/routing-bundle": "self.version",
        "hydface/security-bundle": "self.version",
        "hydface/languages-bundle": "self.version",
        "hydface/translations-bundle": "self.version",
        "hydface/activity-logger-bundle": "self.version",
        "hydface/config-bundle": "self.version",
        "hydface/dynamically-configurable-app-bundle": "self.version",
        "hydface/file-repository-bundle": "self.version",
        "hydface/file-uploader-bundle": "self.version",
        "hydface/mjr-integration-bundle": "self.version",
        "hydface/mjr-theme-integration-bundle": "self.version",
        "hydface/mjr-security-integration-bundle": "self.version",
        "hydface/dynamically-configurable-mjr-bundle": "self.version",
        "hydface/server-crud-bundle": "self.version",
        "hydface/backend-tools-bundle": "self.version",
        "hydface/backend-module-bundle": "self.version",
        "hydface/backend-security-bundle": "self.version",
        "hydface/backend-languages-bundle": "self.version",
        "hydface/backend-translations-tool-bundle": "self.version",
        "hydface/backend-tools-activity-log-bundle": "self.version",
        "hydface/backend-tools-settings-bundle": "self.version",
        "hydface/backend-config-utils-bundle": "self.version",
        "hydface/backend-dashboard-bundle": "self.version",
        "hydface/direct-bundle": "self.version",
        "hydface/mjr-cache-aware-class-loader": "self.version"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.x-dev"
        },
        "modera-monorepo": {
            "include": [
                "src/Modera/*/composer.json"
            ]
        }
    }
}